{
  "name": "injectable-react",
  "description": "Injectable entities for React",
  "version": "0.0.1",
  "private": false,
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "author": "wintpann",
  "repository": {
    "type": "git",
    "url": "https://github.com/wintpann/injectable-react.git"
  },
  "homepage": "https://github.com/wintpann/injectable-react",
  "bugs": {
    "url": "https://github.com/wintpann/injectable-react/issues",
    "email": "vvintpann@gmail.com"
  },
  "license": "MIT",
  "files": [
    "dist/index.js",
    "dist/index.d.ts"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.1",
    "@testing-library/react": "^12.1.4",
    "babel-jest": "^27.5.1",
    "classnames": "^2.3.1",
    "jest": "^27.5.1",
    "np": "^7.6.1",
    "prettier": "^2.7.1",
    "react": "^17.0.2",
    "react-app-rewired": "^2.2.1",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "react-test-renderer": "^17.0.2",
    "react-toastify": "^9.0.7",
    "regenerator-runtime": "^0.13.9",
    "rollup": "^2.70.1",
    "rollup-plugin-terser": "^7.0.2",
    "stats.js": "^0.17.0",
    "uuid": "^8.3.2"
  },
  "scripts": {
    "lint": "prettier --write src --ignore-unknown",
    "dev": "react-app-rewired start",
    "build": "npx rollup --config rollup.config.js && cp index.d.ts dist/",
    "test": "jest",
    "release": "rm -rf dist && yarn lint && yarn build && npx np"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ]
  },
  "keywords": [
    "react",
    "hook",
    "dependency injection",
    "injectable",
    "state"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {}
}
